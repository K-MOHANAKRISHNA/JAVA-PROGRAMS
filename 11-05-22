//stored in notepad Iface.java

package aec.cse.it;
public class Iface
{
	public void CSE()
	{
		System.out.println("CSE Studemts");
	}
	public void IT()
	{
		System.out.println("IT Students");
	}
}
//to save package

C:\Users\vinut>javac -d C:\Users\vinut Iface.java


//stored in Pprog.java

import aec.cse.it.Iface;
class Pprog
{
	public static void main(String args[])
	{
		Iface i=new Iface();
		i.CSE();
		i.IT();
	}
}


output:


C:\Users\vinut>javac Pprog.java

C:\Users\vinut>java Pprog
CSE Studemts
IT Students

//stored in notepad Arithmetic.java
package aec.math;
public class Arithmetic
{
    public int add(int x,int y)
    
{
     return x+y;
}
public int sub(int x,int y)
{
    return x-y;
}
public int mul(int x,int y)
{
   return x*y;
}
public int div(int x,int y)
{
    return x/y;
}
}


C:\Users\vinut>javac -d C:\Users\vinut Arithmetic.java
//stored in Pprog.java


import aec.math.Arithmetic;
class Pprog
{
	public static void main(String args[])
	{
		Arithmetic i=new Arithmetic();
		System.out.println(i.add(2,3));
	}
}


OUTPUT:
C:\Users\vinut>javac Pprog.java

C:\Users\vinut>java Pprog
5
